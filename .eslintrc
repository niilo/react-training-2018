{
  "extends": ["prettier", "kentcdodds", "kentcdodds/react", "kentcdodds/jest"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "camelcase": "off",
    "react/prop-types": "off",
    "react/default-props-match-prop-types": "off",
    "func-names": "off",
    "no-alert": "off",
    "no-unused-vars": "off",
    "no-negated-condition": "off",
    "consistent-return": "off",
    "react/no-unused-prop-types": "off",
    "import/no-useless-path-segments": "off",
    "import/no-unassigned-import": "off",
    "max-lines": "off",
    "no-eq-null": "off",
    "eqeqeq": "off",
    "react/jsx-indent": "off",
    "complexity": ["error", 12]
  }
}
